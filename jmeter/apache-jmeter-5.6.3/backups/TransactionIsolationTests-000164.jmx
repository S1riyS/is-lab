        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Prepare Import File" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import groovy.json.JsonBuilder

            def rnd = new Random()
            def threadNum = ctx.getThreadNum()
            def timestamp = System.currentTimeMillis()

            def colors = [&apos;RED&apos;, &apos;YELLOW&apos;, &apos;ORANGE&apos;,
            &apos;WHITE&apos;, &apos;BROWN&apos;]
            def countries = [&apos;USA&apos;, &apos;SPAIN&apos;, &apos;SOUTH_KOREA&apos;,
            &apos;JAPAN&apos;]
            def ticketTypes = [&apos;VIP&apos;, &apos;USUAL&apos;, &apos;BUDGETARY&apos;,
            &apos;CHEAP&apos;]
            def venueTypes = [&apos;OPEN_AREA&apos;, &apos;CINEMA&apos;, &apos;STADIUM&apos;]

            def numTickets = rnd.nextInt(3) + 1
            def tickets = []
            for (int i = 0; i &lt; numTickets; i++) {
            def coordinates = [
            x: rnd.nextInt(100) - 50,
            y: rnd.nextFloat() * 200 - 100
            ]
            def location = [
            x: rnd.nextInt(10000) - 5000,
            y: rnd.nextInt(100000) + 1,
            z: rnd.nextInt(50000) + 1,
            name: &quot;ImportLocation_${threadNum}_${i}_${timestamp}&quot;
            ]
            def person = [
            eyeColor: colors[rnd.nextInt(colors.size())],
            hairColor: colors[rnd.nextInt(colors.size())],
            location: location,
            passportID:
            &quot;IMPORT_${threadNum}_${i}_${timestamp}&quot;,
            nationality: countries[rnd.nextInt(countries.size())]
            ]
            def event = [
            name: &quot;ImportEvent_${threadNum}_${i}_${timestamp}&quot;,
            date: new Date().format(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ssXXX&quot;),
            minAge: rnd.nextInt(18) + 18,
            description: &quot;Import event ${i}&quot;
            ]
            def venue = [
            name: &quot;ImportVenue_${threadNum}_${i}_${timestamp}&quot;,
            capacity: rnd.nextInt(1000) + 100,
            type: venueTypes[rnd.nextInt(venueTypes.size())]
            ]
            def ticket = [
            name: &quot;ImportTicket_${threadNum}_${i}_${timestamp}&quot;,
            coordinates: coordinates,
            person: person,
            event: event,
            price: rnd.nextDouble() * 1000 + 100,
            type: ticketTypes[rnd.nextInt(ticketTypes.size())],
            discount: rnd.nextDouble() * 20,
            number: rnd.nextDouble() * 100 + 1,
            comment: &quot;Import ticket ${i}&quot;,
            venue: venue
            ]
            tickets.add(ticket)
            }
            // Required format
            def importData = [
            entityType: &quot;TICKET&quot;,
            data: tickets
            ]

            def fileName = &quot;import_tickets_${threadNum}_${timestamp}.json&quot;
            def filePath = &quot;./tmp/${fileName}&quot;
            def file = new File(filePath)
            file.parentFile.mkdirs()
            file.write(new JsonBuilder(importData).toPrettyString())
            vars.put(&quot;import_file_path&quot;, filePath)
            log.info(&quot;Generated import file: ${filePath} with ${numTickets} tickets&quot;)</stringProp>
        </JSR223PreProcessor>
